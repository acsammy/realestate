          <!-- #Item 0 -->
          <!-- Item 1 -->
          <div class="item">
            <%= link_to(image_tag("fama1.jpg"), estates_path)%>
          </div>
          <!-- #Item 1 -->
          <!-- Item 2 -->
          <div class="item">
            <%= link_to(image_tag("fama2.jpg"), estates_path)%>
          </div>
          <!-- #Item 2 -->
